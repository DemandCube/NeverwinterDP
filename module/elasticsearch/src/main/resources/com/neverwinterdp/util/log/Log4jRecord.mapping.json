{
  "Log4jRecord": {
    "properties" : {
      "timestamp" : { 
        "type" : "date", "format" : "dd/MM/yyyy HH:mm:ss" 
      },

      "level" : { "type" : "string", "index":"not_analyzed" },
      "host" : { "type" : "string", "index":"not_analyzed" },
      "appName" : { "type" : "string", "index":"not_analyzed" },
      "loggerName" : { "type" : "string", "index": "not_analyzed" }  
    }
  }
}
